{
  "name": "@dwalter/spider-dev-middleware",
  "version": "1.7.0",
  "sideEffects": false,
  "description": "developer middleware for spider-store",
  "keywords": [
    "devTools",
    "freeze",
    "persist",
    "spider",
    "middleware"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/spider-web.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "@dwalter/spider-store": "^1.7.0"
  },
  "peerDependencies": {
    "@dwalter/spider-store": "^1.7.0"
  }
}
