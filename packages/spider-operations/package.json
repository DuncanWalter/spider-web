{
  "name": "@dwalter/spider-operations",
  "version": "1.4.5",
  "sideEffects": false,
  "description": "waste free state management",
  "keywords": [
    "state",
    "hooks",
    "observable",
    "store"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/spider-web.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "peerDependencies": {
    "@dwalter/spider-store": "^1.4.0"
  },
  "devDependencies": {
    "@dwalter/create-reducer": "^1.0.1",
    "@dwalter/spider-store": "^1.4.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "rollup": "^0.67.3",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  }
}
