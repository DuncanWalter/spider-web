{
  "name": "@dwalter/spider-operations",
  "version": "1.7.0",
  "sideEffects": false,
  "description": "waste free state management",
  "keywords": [
    "state",
    "hooks",
    "observable",
    "store"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/spider-web.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  }
}
