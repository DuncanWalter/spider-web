{
  "name": "@dwalter/spider-store",
  "version": "2.0.4",
  "sideEffects": false,
  "description": "waste free state management",
  "keywords": [
    "state",
    "hooks",
    "observable",
    "store"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": "https://github.com/DuncanWalter/spider-web/tree/master/packages/spider-store",
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web/packages/spider-store#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "@dwalter/create-reducer": "^2.0.4"
  }
}
