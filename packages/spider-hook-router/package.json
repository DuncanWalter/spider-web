{
  "name": "@dwalter/spider-hook-router",
  "version": "1.4.5",
  "sideEffects": false,
  "description": "hook based routing for use with spider-hook",
  "keywords": [
    "route",
    "hook",
    "spider"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": "https://github.com/DuncanWalter/spider-web/tree/master/packages/spider-hook-router",
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web/packages/spider-hook-router#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "peerDependencies": {
    "@dwalter/spider-hook": "^1.4.5",
    "history": "^4.9.0",
    "path-to-regexp": "^3.0.0"
  },
  "devDependencies": {
    "@dwalter/create-reducer": "^1.0.1",
    "@dwalter/spider-hook": "^1.4.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^16.8.6",
    "history": "^4.9.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "path-to-regexp": "^3.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-testing-library": "^5.4.2",
    "rollup": "^0.67.3",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  }
}
