{
  "name": "@dwalter/spider-hook",
  "version": "2.0.3",
  "sideEffects": false,
  "description": "state management for react powered by spider-store",
  "keywords": [
    "state",
    "hooks",
    "observable",
    "store"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.tsx",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": "https://github.com/DuncanWalter/spider-web/tree/master/packages/spider-hook",
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web/packages/spider-hook#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "@dwalter/spider-store": "^2.0.1"
  },
  "devDependencies": {
    "@dwalter/create-reducer": "^2.0.1"
  }
}
