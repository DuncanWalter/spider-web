{
  "name": "@dwalter/spider-hook",
  "version": "1.3.2",
  "description": "waste free state management",
  "keywords": [
    "state",
    "hooks",
    "observable",
    "store"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/spider-web.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "dependencies": {
    "@dwalter/spider-store": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.7.7",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "react": "16.7.0-alpha.0 - next",
    "react-dom": "16.7.0-alpha.0 - next",
    "react-testing-library": "^5.4.2",
    "rollup": "^0.67.3",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  }
}
