  "private": true,
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/react": "16.8.6",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "husky": "1.2.0",
    "jest": "24.9.0",
    "lerna": "3.13.3",
