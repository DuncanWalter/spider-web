{
  "name": "spider-web",
  "version": "1.0.0",
  "description": "waste free state management",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*.js",
    "**/*.ts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "tsc --emitDeclarationOnly --declaration --declarationDir types && npm run build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/spider-web.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/spider-web/issues"
  },
  "homepage": "https://github.com/DuncanWalter/spider-web#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "rollup": "^0.67.3",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "flatqueue": "^1.1.0"
  }
}
